
auto lo
iface lo inet loopback

auto eth0
iface eth0
  address 192.168.1.202/24


{% for iface in openconfig_interfaces.interface %}
auto {{ iface.name }}
iface {{ iface.name }}
    alias "{{ iface.config.description }}";
{% for unit in iface.config.subinterfaces.subinterface %}
{% for address in unit.ipv4.addresses.address %}
    address {{ address.ip }}
{%endfor %}
{%endfor%}
{%endfor%}
